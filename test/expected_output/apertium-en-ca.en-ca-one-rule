macro_determiner_end: ret
#<def-macro npar="2" n="f_concord2">
macro_f_concord2_start:
#<clip part="nbr" pos="1" side="tl">
push 1
push "<sg>|<pl>|<sp>|<ND>"
cliptl
#<lit-tag v="ND">
push "<ND>"
cmp
jz when_4_end
#<clip part="nbr" pos="2" side="tl">
push 2
push "<sg>|<pl>|<sp>|<ND>"
cliptl
#<lit-tag v="sg">
push "<sg>"
cmp
#<clip part="nbr" pos="2" side="tl">
push 2
push "<sg>|<pl>|<sp>|<ND>"
cliptl
#<lit-tag v="pl">
push "<pl>"
cmp
or 2
jz when_5_end
#<clip part="nbr" pos="1" side="tl">
push 1
push "<sg>|<pl>|<sp>|<ND>"
#<clip part="nbr" pos="2" side="tl">
push 2
push "<sg>|<pl>|<sp>|<ND>"
cliptl
storetl
jmp choose_4_end
when_5_end:
#<clip part="a_num" pos="2" side="tl">
push 2
push "<num>|<num><percent>|<num><ord>|<num><mon>"
cliptl
#<lit-tag v="num">
push "<num>"
cmp
jz when_6_end
#<clip part="nbr" pos="1" side="tl">
push 1
push "<sg>|<pl>|<sp>|<ND>"
#<lit-tag v="pl">
push "<pl>"
storetl
jmp choose_4_end
when_6_end:
choose_4_end:
jmp choose_3_end
when_4_end:
choose_3_end:
macro_f_concord2_end: ret
#<def-macro npar="2" n="det_no">
